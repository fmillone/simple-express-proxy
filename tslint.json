{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [
      true,
      "always"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "member-ordering": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "interface-name": false,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "member-access": [true, "no-public"],
    "arrow-parens": {
      "options": ["ban-single-arg-parens"]
    },
    "no-console": false
  },
  "rulesDirectory": []
}
